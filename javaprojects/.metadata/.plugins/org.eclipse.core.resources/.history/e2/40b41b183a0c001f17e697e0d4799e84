package switchcaseproject;

public class Main3 {

	public static void main(String[] args) {
		Month mon = Month.JUNE;
		showdayCount(mon);

	}

	static void showdayCount(Month m) {

		switch (m) {

		case NOVEMBER, DECEMBER, JANUARY, FEBRUARY:
			System.out.println("Very Cold");

		case MARCH, APRIL:
			System.out.println("Not very cold");
		case MAY, JUNE, JULY:
			System.out.println("Very hot");

		case AUGUST, SEPTEMBER, OCTOBER:
			System.out.println("It rains");

		}
		;

		System.out.println(noOfDays);

	}
}