package com.inherit;

public class InheritMain {

	public static void main(String[] args) {

		Person p;
Employee e;
Student s;
		p = new Person("Some name", "Mumbai");
		p.displayDetails(); // displayDetails() of Person class

		System.out.println("====================================");
		
		e = new Employee("Srinidhi", "Hyderabad", 100, 34000);
		e.displayDetails(); // displayDetails(0 of Employee class
		
		System.out.println("====================================");
		
		s = new Student("Sudhir", "Chennai", 11, 98.2); //displayDetails() of Student class
		s.displayDetails();

	}

}
