package switchcaseproject;

public class Main3 {

	public static void main(String[] args) {
		Month mon = Month.JUNE;
		showdayCount(mon);

	}

	static void showdayCount(Month m) {

		switch (m) {

		case NOVEMBER, DECEMBER, JANUARY, FEBRUARY:
			System.out.println("Very Cold"); break;

		case MARCH, APRIL:
			System.out.println("Not very cold"); break;
		case MAY, JUNE, JULY:
			System.out.println("Very hot"); break;

		case AUGUST, SEPTEMBER, OCTOBER:
			System.out.println("It rains");

		}
		

	}
}