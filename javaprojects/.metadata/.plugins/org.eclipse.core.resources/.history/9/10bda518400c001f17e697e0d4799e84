package com.inherit;

public class InheritMain {

	public static void main(String[] args) {

		Employee e = new Employee( "Srinidhi","Hyderabad",100, 34000);

		System.out.println(e.getName()); // calling super class method

		System.out.println(e.getSalary()); // calling its own method

		e.display(); // calling super class method
	}

}
