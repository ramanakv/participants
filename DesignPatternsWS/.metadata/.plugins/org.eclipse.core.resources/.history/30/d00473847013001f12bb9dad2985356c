package statepatternproject;

import java.util.Scanner;

public class StateMain {

	public static void main(String[] args) {
		
		CeilingFan fan = new CeilingFan();
		Scanner sc = new Scanner(System.in);
		
		while(true) {
			System.out.println("Press Enter");
			sc.nextLine();
			fan.pullChain();
		}

	}

}
